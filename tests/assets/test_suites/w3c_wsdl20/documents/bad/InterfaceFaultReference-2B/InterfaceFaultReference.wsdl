<wsdl:description targetNamespace="http://example.org/"
	xmlns:tns="http://example.org/"
	xmlns:wsdl="http://www.w3.org/ns/wsdl"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<wsdl:documentation>
		This test shows a invalid use of WSDL 2.0 interface 
		fault reference. The combination of the fault 
		reference name and fault reference must be unique for
		the interface operation.
		
		This document violates assertion 
		InterfaceFaultReference-1039.
	</wsdl:documentation>
	
	<wsdl:types>
		<xs:schema targetNamespace="http://example.org">
			<xs:element name="myElement" type="xs:string" />
		</xs:schema>
	</wsdl:types>
	
	<wsdl:interface name="myInterface">
		<wsdl:operation name="myOperation" pattern="http://www.w3.org/ns/wsdl/robust-in-only">
			<wsdl:input element="tns:myElement"/>
			<wsdl:outfault messageLabel="myOutFault" ref="tns:myFault"/>
			<wsdl:outfault messageLabel="myOutFault" ref="tns:myFault"/>
		</wsdl:operation>
		<wsdl:fault name="myFault" element="tns:myElement"></wsdl:fault>
	</wsdl:interface>

</wsdl:description>
