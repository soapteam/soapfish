<wsdl:description targetNamespace="http://example.org/Service"
	xmlns:tns="http://example.org/Service"
	xmlns:wsdl="http://www.w3.org/ns/wsdl"
	xmlns:wsoap="http://www.w3.org/ns/wsdl/soap"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<wsdl:documentation>
		This test shows an invalid use of a Service. An empty address is
		specified of the endpoint.

		This document violates assertion Endpoint-1061.
	</wsdl:documentation>

	<wsdl:interface name="EchoInterface">
		<wsdl:operation name="Echo"
			pattern="http://www.w3.org/ns/wsdl/in-out">
			<wsdl:input element="#any" />
			<wsdl:output element="#any" />
		</wsdl:operation>
	</wsdl:interface>

	<wsdl:binding name="EchoSOAPBinding"
		type="http://www.w3.org/ns/wsdl/soap"
		wsoap:protocol="http://www.w3.org/2003/05/soap/bindings/HTTP/" />

	<wsdl:service name="EchoService" interface="tns:EchoInterface">
		<wsdl:endpoint name="SOAPEndpoint" binding="tns:EchoSOAPBinding"
			address="" />
	</wsdl:service>
</wsdl:description>
